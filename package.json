{
    "name": "genies-hud-manager",
    "version": "1.0.0-dev",
    "main": "./dist/electron/main.js",
    "author": "Marlon Leoner <marlon.leoner@gmail.com>",
    "description": "Genies HUD Manager é uma ferramenta projetada para simplificar o gerenciamento de HUDs (Heads-Up Displays) no jogo Counter-Strike 2. O Genies permite aos jogadores personalizar facilmente a aparência e o layout dos elementos visuais do jogo, como mapas, contadores de saúde e munição, além de outras informações cruciais durante a partida.",
    "scripts": {
        "ng": "ng",
        "dev:app": "tsc -p tsconfig.api.json --watch",
        "dev:front": "ng build --watch --configuration development",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.1.0",
        "@angular/common": "^17.1.0",
        "@angular/compiler": "^17.1.0",
        "@angular/core": "^17.1.0",
        "@angular/forms": "^17.1.0",
        "@angular/platform-browser": "^17.1.0",
        "@angular/platform-browser-dynamic": "^17.1.0",
        "@angular/router": "^17.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.1.0",
        "@angular/cli": "^17.1.0",
        "@angular/compiler-cli": "^17.1.0",
        "@types/jasmine": "~5.1.0",
        "autoprefixer": "^10.4.18",
        "electron": "^29.1.1",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "typescript": "~5.3.2"
    }
}
